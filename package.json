{
  "name": "automatik-website",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:client-and-server": "ng build -c staging && ng run automatik-website:server:staging",
    "build:client-and-server-test": "ng build -c test && ng run automatik-website:server:test",
    "build:client-and-server-production": "ng build --prod && ng run automatik-website:server:production",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr": "npm run build:client-and-server && npm run webpack:server",
    "build:ssr-test": "npm run build:client-and-server-test && npm run webpack:server",
    "build:ssr-production": "npm run build:client-and-server-production && npm run webpack:server",
    "serve:ssr": "sudo node dist/server",
    "forever:ssr": "sudo forever start dist/server",
    "sendit": "npm run build:ssr && npm run serve:ssr",
    "sendit-test": "npm run build:ssr-test && npm run serve:ssr",
    "sendit-forever": "npm run build:ssr-production && npm run forever:ssr"
  },
  "private": true,
  "dependencies": {
